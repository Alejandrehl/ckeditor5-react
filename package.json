{
  "name": "@ckeditor/ckeditor5-react",
  "version": "0.1.0",
  "description": "Official CKEditor 5 React component.",
  "main": "dist/ckeditor.js",
  "keywords": [
    "ckeditor",
    "ckeditor5",
    "ckeditor 5",
    "ckeditor5-feature",
    "react"
  ],
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2"
  },
  "devDependencies": {
    "@ckeditor/ckeditor5-dev-env": "^11.1.1",
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1"
  },
  "scripts": {
    "build": "babel --out-dir ./dist ./src --source-maps",
    "develop": "babel --out-dir ./dist ./src --source-maps --watch",
    "preversion": "npm run build; if [ -n \"$(git status dist/ --porcelain)\" ]; then git add -u build/ && git commit -m 'Internal: Build.'; fi",
    "changelog": "node ./scripts/changelog.js",
    "release": "node ./scripts/release.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ckeditor/ckeditor5-react.git"
  },
